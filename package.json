{
	"name": "url-shortener",
	"version": "1.0.0",
	"description": "Shortens the URL",
	"main": "server.js",
	"scripts": {
		"start": "node server.js",
		"dev": "nodemon server.js",
		"production": "NODE_ENV=production nodemon server.js",
		"watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
		"build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/karmanya007/url-shortener.git"
	},
	"keywords": [
		"node",
		"express",
		"ejs",
		"mongoDB",
		"mongoose"
	],
	"author": "Karmanya Veer Sharma",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/karmanya007/url-shortener/issues"
	},
	"homepage": "https://github.com/karmanya007/url-shortener#readme",
	"dependencies": {
		"@babel/polyfill": "^7.11.5",
		"axios": "^0.20.0",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"ejs": "^3.1.5",
		"express": "^4.17.1",
		"express-mongo-sanitize": "^2.0.0",
		"express-rate-limit": "^5.1.3",
		"fnv-plus": "^1.3.1",
		"helmet": "^4.1.1",
		"hpp": "^0.2.3",
		"mongoose": "^5.10.7",
		"morgan": "^1.10.0",
		"nanoid": "^3.1.12",
		"validator": "^13.1.17",
		"xss-clean": "^0.1.1"
	},
	"devDependencies": {
		"parcel-bundler": "^1.12.4"
	},
	"engines": {
		"node": "12.x"
	}
}
